{
  "name": "4all",
  "version": "1.0.0",
  "description": "A simple project to test my skills",
  "main": "src/server.js",
  "scripts": {
    "build": "tsc",
    "start": "tsc; node dist/server;",
    "prettier": "prettier 'src/**/*.ts' --write",
    "lint": "eslint 'src/**/*.ts' --ext .ts",
    "test": "mocha -r ts-node/register src/**/*.test.ts --recursive",
    "coverage": "nyc -e .ts npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoseLeandroSantosJustin/4all.git"
  },
  "author": "Jose Leandro Santos Justin",
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run coverage"
    }
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/sinon": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.7",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "sinon": "^9.2.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "winston": "^3.3.3"
  }
}
